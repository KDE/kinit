#: kdeinit/kinit.cpp:526
msgid ""
"Unable to start new process.\n"
"The system may have reached the maximum number of open files possible or the "
"maximum number of open files that you are allowed to use has been reached."
msgstr ""
"无法启动新进程。\n"
"系统已打开的文件数可能已经达到了极限，或者您所允许打开的文件数已经达到了极"
"限。"

#: kdeinit/kinit.cpp:551
msgid ""
"Unable to create new process.\n"
"The system may have reached the maximum number of processes possible or the "
"maximum number of processes that you are allowed to use has been reached."
msgstr ""
"无法创建新进程。\n"
"系统已创建的进程数可能已经达到了极限，或者您所允许创建进程数已经达到了极限。"

#: kdeinit/kinit.cpp:642
#, kde-format
msgid "Could not find '%1' executable."
msgstr "找不到可执行程序“%1”。"

#: kdeinit/kinit.cpp:670
#, kde-format
msgid ""
"Could not open library '%1'.\n"
"%2"
msgstr ""
"无法打开库“%1”。\n"
"%2"

#: kdeinit/kinit.cpp:712
#, kde-format
msgid ""
"Could not find 'kdemain' in '%1'.\n"
"%2"
msgstr ""
"无法在“%1”中找到“kdemain”。\n"
"%2"

#: klauncher/klauncher.cpp:485
#, kde-format
msgid "KDEInit could not launch '%1'"
msgstr "KDEInit 无法启动“%1”"

#: klauncher/klauncher.cpp:678 klauncher/klauncher.cpp:692
#, kde-format
msgid "Could not find service '%1'."
msgstr "找不到服务“%1”。"

#: klauncher/klauncher.cpp:710
#, kde-format
msgid "Service '%1' must be executable to run."
msgstr "服务“%1”必须可执行方能运行。"

#: klauncher/klauncher.cpp:712 klauncher/klauncher.cpp:753
#, kde-format
msgid "Service '%1' is malformatted."
msgstr "服务“%1”的格式不正确。"

#: klauncher/klauncher.cpp:847
#, kde-format
msgid "Launching %1"
msgstr "调用 %1"

#: klauncher/klauncher.cpp:1049
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "未知的协议“%1”。\n"

#: klauncher/klauncher.cpp:1055
#, kde-format
msgid "Could not find the '%1' plugin.\n"
msgstr "找不到插件“%1”。\n"

#: klauncher/klauncher.cpp:1122
#, kde-format
msgid "Error loading '%1'.\n"
msgstr "装入“%1”时出错。\n"

#: klauncher/klauncher_main.cpp:142
msgid ""
"klauncher: This program is not supposed to be started manually.\n"
"klauncher: It is started automatically by kdeinit5.\n"
msgstr ""
"klauncher: 这个程序不应该手工执行。\n"
"klauncher: 它会被 kdeinit5 自动启动。\n"
