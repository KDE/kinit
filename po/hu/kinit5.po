#: kdeinit/kinit.cpp:500
#, kde-format
msgid ""
"Unable to start new process.\n"
"The system may have reached the maximum number of open files possible or the "
"maximum number of open files that you are allowed to use has been reached."
msgstr ""
"Nem sikerült új folyamatot indítani.\n"
"Lehet, hogy a felhasználóra vagy a rendszerre beállított korlátozás miatt "
"már nem nyitható meg több fájl."

#: kdeinit/kinit.cpp:522
#, kde-format
msgid ""
"Unable to create new process.\n"
"The system may have reached the maximum number of processes possible or the "
"maximum number of processes that you are allowed to use has been reached."
msgstr ""
"Nem sikerült új folyamatot létrehozni.\n"
"Lehet, hogy a felhasználóra vagy a rendszerre beállított korlátozás miatt "
"már nem indítható több folyamat."

#: kdeinit/kinit.cpp:605
#, kde-format
msgid "Could not find '%1' executable."
msgstr "A(z) „%1” program nem található."

#: kdeinit/kinit.cpp:638
#, kde-format
msgid ""
"Could not open library '%1'.\n"
"%2"
msgstr ""
"Nem sikerült megnyitni a(z) %1 programkönyvtárt.\n"
"%2"

#: kdeinit/kinit.cpp:675
#, kde-format
msgid ""
"Could not find 'kdemain' in '%1'.\n"
"%2"
msgstr ""
"Nem található „kdemain” függvény ebben: „%1”.\n"
"%2"

#: klauncher/klauncher.cpp:418
#, kde-format
msgid "KDEInit could not launch '%1'"
msgstr "A KDEInit nem tudta elindítani ezt a programot: „%1”"

#: klauncher/klauncher.cpp:599 klauncher/klauncher.cpp:613
#, kde-format
msgid "Could not find service '%1'."
msgstr "A(z) „%1” szolgáltatás nem található."

#: klauncher/klauncher.cpp:631
#, kde-format
msgid "Service '%1' must be executable to run."
msgstr "A szolgáltatás („%1”) programfájljának végrehajthatónak kell lennie."

#: klauncher/klauncher.cpp:633 klauncher/klauncher.cpp:694
#, kde-format
msgid "Service '%1' is malformatted."
msgstr "A(z) „%1” szolgáltatás hibás."

#: klauncher/klauncher.cpp:789
#, kde-format
msgid "Launching %1"
msgstr "%1 indítása"

#: klauncher/klauncher.cpp:970
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "Ismeretlen protokoll - „%1”.\n"

#: klauncher/klauncher.cpp:976
#, kde-format
msgid "Could not find the '%1' plugin.\n"
msgstr "A(z) „%1” bővítmény nem található.\n"

#: klauncher/klauncher.cpp:1049
#, kde-format
msgid "Error loading '%1'."
msgstr "Hiba történt a(z) „%1” betöltése közben."

#: klauncher/klauncher_main.cpp:136
#, kde-format
msgid ""
"klauncher: This program is not supposed to be started manually.\n"
"klauncher: It is started automatically by kdeinit5.\n"
msgstr ""
"klauncher: ezt a programot nem kézzel kell elindítani.\n"
"klauncher: a kdeinit5 fogja automatikusan elindítani.\n"
