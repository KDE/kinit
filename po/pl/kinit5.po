#: kdeinit/kinit.cpp:512
#, kde-format
msgid ""
"Unable to start new process.\n"
"The system may have reached the maximum number of open files possible or the "
"maximum number of open files that you are allowed to use has been reached."
msgstr ""
"Nie można rozpocząć nowego procesu.\n"
"Być może osiągnięto maksymalną liczbę plików otwartych w systemie albo "
"maksymalną liczbę plików otwartych przez użytkownika."

#: kdeinit/kinit.cpp:534
#, kde-format
msgid ""
"Unable to create new process.\n"
"The system may have reached the maximum number of processes possible or the "
"maximum number of processes that you are allowed to use has been reached."
msgstr ""
"Nie można utworzyć nowego procesu.\n"
"Być może osiągnięto maksymalną liczbę procesów w systemie albo maksymalną "
"liczbę procesów użytkownika."

#: kdeinit/kinit.cpp:617
#, kde-format
msgid "Could not find '%1' executable."
msgstr "Nie można odnaleźć pliku wykonywalnego '%1'."

#: kdeinit/kinit.cpp:650
#, kde-format
msgid ""
"Could not open library '%1'.\n"
"%2"
msgstr ""
"Nie można otworzyć biblioteki '%1'.\n"
"%2"

#: kdeinit/kinit.cpp:687
#, kde-format
msgid ""
"Could not find 'kdemain' in '%1'.\n"
"%2"
msgstr ""
"Nie można znaleźć 'kdemain' w '%1'.\n"
"%2"

#: klauncher/klauncher.cpp:431
#, kde-format
msgid "KDEInit could not launch '%1'"
msgstr "KDEInit nie może uruchomić '%1'"

#: klauncher/klauncher.cpp:612 klauncher/klauncher.cpp:626
#, kde-format
msgid "Could not find service '%1'."
msgstr "Nie można znaleźć usługi '%1'."

#: klauncher/klauncher.cpp:644
#, kde-format
msgid "Service '%1' must be executable to run."
msgstr "Usługa '%1' musi być wykonywalna, żeby można ją było uruchomić."

#: klauncher/klauncher.cpp:646 klauncher/klauncher.cpp:707
#, kde-format
msgid "Service '%1' is malformatted."
msgstr "Usługa '%1' jest zniekształcona."

#: klauncher/klauncher.cpp:802
#, kde-format
msgid "Launching %1"
msgstr "Uruchamianie %1"

#: klauncher/klauncher.cpp:983
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "Nieznany protokół '%1'.\n"

#: klauncher/klauncher.cpp:989
#, kde-format
msgid "Could not find the '%1' plugin.\n"
msgstr "Nie można znaleźć wtyczki '%1'.\n"

#: klauncher/klauncher.cpp:1062
#, kde-format
msgid "Error loading '%1'."
msgstr "Błąd wczytywania '%1'."

#: klauncher/klauncher_main.cpp:148
#, kde-format
msgid ""
"klauncher: This program is not supposed to be started manually.\n"
"klauncher: It is started automatically by kdeinit5.\n"
msgstr ""
"klauncher: ten program nie jest przeznaczony do samodzielnego działania.\n"
"klauncher: program jest wykorzystywany przez kdeinit5.\n"
