msgstr "תבנית השירות \"%1\" שגויה."

#: klauncher/klauncher.cpp:784
#, kde-format
msgid "Launching %1"
msgstr "מפעיל את %1"

#: klauncher/klauncher.cpp:965
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "פרוטוקול לא מוכר \"%1\".\n"

#: klauncher/klauncher.cpp:971
#, fuzzy, kde-format
