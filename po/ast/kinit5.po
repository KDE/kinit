#: kdeinit/kinit.cpp:496
#, kde-format
msgid ""
"Unable to start new process.\n"
"The system may have reached the maximum number of open files possible or the "
"maximum number of open files that you are allowed to use has been reached."
msgstr ""
"Nun pue aniciase'l procesu nuevu.\n"
"El sistema pudo algamar el númberu máximu de ficheros abiertos o'l númberu "
"de ficheros abiertos que tienes permisu pa usar."

#: kdeinit/kinit.cpp:518
#, kde-format
msgid ""
"Unable to create new process.\n"
"The system may have reached the maximum number of processes possible or the "
"maximum number of processes that you are allowed to use has been reached."
msgstr ""
"Nun pue crease'l procesu nuevu.\n"
"El sistema pudo algamar el númberu máximu de procesos posibles o'l númberu "
"de procesos que tienes permisu pa usar."

#: kdeinit/kinit.cpp:601
#, kde-format
msgid "Could not find '%1' executable."
msgstr "Nun pudo atopase l'executable «%1»."

#: kdeinit/kinit.cpp:634
#, kde-format
msgid ""
"Could not open library '%1'.\n"
"%2"
msgstr ""
"Nun pudo abrise la biblioteca «%1».\n"
"%2"

#: kdeinit/kinit.cpp:671
#, kde-format
msgid ""
"Could not find 'kdemain' in '%1'.\n"
"%2"
msgstr ""
"Nun pudo atopase «kdemain» en «%1».\n"
"%2"

#: klauncher/klauncher.cpp:418
#, kde-format
msgid "KDEInit could not launch '%1'"
msgstr "KDEInit nun pudo llanzar «%1»"

#: klauncher/klauncher.cpp:599 klauncher/klauncher.cpp:613
#, kde-format
msgid "Could not find service '%1'."
msgstr "Nun pudo atopase'l ficheru «%1»."

#: klauncher/klauncher.cpp:631
#, kde-format
msgid "Service '%1' must be executable to run."
msgstr "El serviciu «%1» ha ser executable pa executase."

#: klauncher/klauncher.cpp:633 klauncher/klauncher.cpp:694
#, kde-format
msgid "Service '%1' is malformatted."
msgstr "El serviciu «%1» tien un formatu malu."

#: klauncher/klauncher.cpp:789
#, kde-format
msgid "Launching %1"
msgstr "Llanzando %1"

#: klauncher/klauncher.cpp:970
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "Desconozse'l protocolu «%1».\n"

#: klauncher/klauncher.cpp:976
#, kde-format
msgid "Could not find the '%1' plugin.\n"
msgstr "Nun pudo atopase'l plugin «%1».\n"

#: klauncher/klauncher.cpp:1049
#, kde-format
msgid "Error loading '%1'."
msgstr "Fallu al cargar «%1»."

#: klauncher/klauncher_main.cpp:136
#, kde-format
msgid ""
"klauncher: This program is not supposed to be started manually.\n"
"klauncher: It is started automatically by kdeinit5.\n"
msgstr ""
"klauncher: Esti programa nun ta pensáu p'anicialu a mano.\n"
"klauncher: Anicióse automáticamente por kdeinit5.\n"
